  "description": "Seed for cordova project",
  "main": "index.js",
  "scripts": {
    "preinstall": "npm install tsd --save-dev && tsd install",
    "test": "NODE_ENV=test karma start",
    "start": "webpack-dev-server --progress --colors",
    "build": "webpack -p --progress --colors",
    "preandroid": "test -d platforms/android || cordova platform add android",
